<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.quancheng.saluki.monitor.mapper.DubboInvokeMapper">
 
    <sql id="allNoAsOfDubboInvoke">
        id,
        invoke_date,
        service,
        method,
        consumer,
        provider,
        type,
        invoke_time,
        success,
        failure,
        elapsed,
        concurrent,
        inPutParam,
        outPutParam
    </sql>

    <!-- Add -->
    <insert id="addDubboInvoke" parameterType="com.quancheng.saluki.monitor.domain.DubboInvoke">
        insert into
        saluki_invoke (
        <include refid="allNoAsOfDubboInvoke" />
        )
        values(
        #{id},
        #{invokeDate},
        #{service},
        #{method},
        #{consumer},
        #{provider},
        #{type},
        #{invokeTime},
        #{success},
        #{failure},
        #{elapsed},
        #{concurrent},
        #{inPutParam},
        #{outPutParam}
        )
    </insert>
</mapper>